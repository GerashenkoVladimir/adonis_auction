{
  "name": "adonis-api-app",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Adonisjs boilerplate for API server with pre-configured JWT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "test": "node $NODE_DEBUG_OPTION ace test",
    "startDev": "nodemon --inspect server.js",
    "standardAutofix": "node ./node_modules/standard/bin/cmd.js --fix"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.2",
    "@adonisjs/auth": "^3.0.6",
    "@adonisjs/bodyparser": "^2.0.3",
    "@adonisjs/cors": "^1.0.6",
    "@adonisjs/fold": "^4.0.8",
    "@adonisjs/framework": "^5.0.7",
    "@adonisjs/ignitor": "^2.0.6",
    "@adonisjs/lucid": "^5.0.4",
    "@adonisjs/validator": "^5.0.3",
    "@adonisjs/vow": "^1.0.16",
    "jsonapi-serializer": "^3.5.6",
    "moment": "^2.22.2",
    "pg": "^7.4.3"
  },
  "devDependencies": {
    "nodemon": "^1.18.3",
    "standard": "^11.0.1"
  },
  "autoload": {
    "App": "./app"
  },
  "standard": {
    "globals": [
      "use",
      "expect",
      "ace"
    ]
  }
}
